{
    "name": "evheniy/html5-verti-template-bundle",
    "description": "The HTML5VertiTemplateBundle adds the ability to add free responsive site template by HTML5 to your application.",
    "keywords": ["HTML5", "Template", "Symfony", "Verty", "Responsive"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "evheniy",
            "homepage": "https://github.com/evheniy"
        }
    ],
    "require": {
        "php":             ">=5.3.0",
        "symfony/symfony": ">=2.4",
        "symfony/assetic-bundle": "~2.5-dev",
        "evheniy/jquery-bundle": ">=1.0",
        "evheniy/html5-cache-bundle": ">=1.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Evheniy\\HTML5VertiTemplateBundle": ""
        }
    },
    "target-dir" : "Evheniy/HTML5VertiTemplateBundle"
}
