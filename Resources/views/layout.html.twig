                                    <li><a href="#">Etiam dolore nisl</a></li>
                                </ul>
                            </li>
                            <li><a href="#">Veroeros feugiat</a></li>
                        </ul>
                    </li>
                    <li{%- if app.request.attributes.get('_route') == 'HTML5_verti_template_bundle_demo_left_sidebar' %} class="current"{%- endif -%}>
                        <a href="{{- path('HTML5_verti_template_bundle_demo_left_sidebar') -}}">Left Sidebar</a>
                    </li>
                    <li{%- if app.request.attributes.get('_route') == 'HTML5_verti_template_bundle_demo_right_sidebar' %} class="current"{%- endif -%}>
                        <a href="{{- path('HTML5_verti_template_bundle_demo_right_sidebar') -}}">Right Sidebar</a>
                    </li>
                    <li{%- if app.request.attributes.get('_route') == 'HTML5_verti_template_bundle_demo_no_sidebar' %} class="current"{%- endif -%}>
                        <a href="{{- path('HTML5_verti_template_bundle_demo_no_sidebar') -}}">No Sidebar</a>
                    </li>
                </ul>
            </nav>

        </header>
    </div>
{%- endblock -%}
